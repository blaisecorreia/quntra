<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 180 40" width="180" height="40">
  <!-- Icon: stylized Q with chart bars -->
  <g transform="translate(0, 4)">
    <!-- Bar chart bars rising (like trading/finance) -->
    <rect x="2" y="20" width="5" height="12" rx="1.5" fill="#0FEDBE"/>
    <rect x="9" y="13" width="5" height="19" rx="1.5" fill="#FDD458"/>
    <rect x="16" y="7" width="5" height="25" rx="1.5" fill="#5862FF"/>
    <rect x="23" y="2" width="5" height="30" rx="1.5" fill="#E8BA40"/>
    <!-- Upward trend line over bars -->
    <polyline points="4,20 11,13 18,7 25,2" fill="none" stroke="#FF8243" stroke-width="1.8" stroke-linecap="round" stroke-linejoin="round" opacity="0.85"/>
  </g>

  <!-- Wordmark: Quntra -->
  <text
    x="36"
    y="27"
    font-family="Georgia, 'Times New Roman', serif"
    font-size="22"
    font-weight="700"
    letter-spacing="-0.5"
    fill="#F5F5F5"
  >QUNTRA</text>
</svg>
